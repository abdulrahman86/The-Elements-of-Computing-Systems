;
; BIND data file for local loopback interface
;
$TTL	3600
@	IN	SOA	ns2.huvu.ru. support.huvu.ru. (
		     2017071401		; Serial
			   1250		; Refresh
			    130		; Retry
			1209600		; Expire
			   3600 )	; Negative Cache TTL
;

@       IN      NS      ns2.
4       IN      PTR     ns2.huvu.ru.
5       IN      PTR     huvu.ru.
